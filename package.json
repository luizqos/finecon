{
    "name": "finecon",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "fb": "concurrently -n \"BACK,FRONT\" -c \"blue,magenta\" \"npx tsx server.ts\" \"npm start\"",
        "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
        "build": "next build && npm run build:server",
        "start": "node dist/server.js & node server.js",
        "start:front": "node dist/server.js",
        "start:back": "npx tsx server.ts",
        "preflight": "tsx src/scripts/preflight.ts",
        "prebuild": "npm run preflight"
    },
    "dependencies": {
        "bootstrap": "^5.3.8",
        "bootstrap-icons": "^1.13.1",
        "canvas-confetti": "^1.9.4",
        "cors": "^2.8.5",
        "csv-parse": "^6.1.0",
        "csv-parser": "^3.2.0",
        "exceljs": "^4.4.0",
        "express": "^5.2.1",
        "lucide-react": "^0.562.0",
        "multer": "^2.0.2",
        "next": "16.1.2",
        "nextjs-toast-notify": "^1.58.0",
        "pino": "^10.3.0",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-hot-toast": "^2.6.0"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4",
        "@types/canvas-confetti": "^1.9.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/multer": "^2.0.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "concurrently": "^9.2.1",
        "eslint": "^9",
        "eslint-config-next": "16.1.2",
        "pino-pretty": "^13.1.3",
        "tailwindcss": "^4",
        "tsc-alias": "^1.8.16",
        "tsx": "^4.21.0",
        "typescript": "^5"
    }
}
